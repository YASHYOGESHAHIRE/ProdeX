{
    "version": 2,
    "builds": [
      {
        "src": "code1.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|webp|ico))",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "code1.js"
      }
    ],
    "installCommand": "apt-get update && apt-get install -y libcairo2-dev libpango1.0-dev libjpeg-dev libgif-dev librsvg2-dev libpixman-1-dev && npm ci"
  }
  